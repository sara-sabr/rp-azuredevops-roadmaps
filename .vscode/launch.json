{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Edge",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "https://localhost:3000/",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "trace": false,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*"
      }
    },
    {
      "name": "Attach to Edge",
      "type": "pwa-msedge",
      "request": "attach",
      "url": "https://localhost:3000/",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "trace": false
    }
  ]
}
